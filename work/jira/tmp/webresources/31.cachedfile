/* module-key = 'com.pyxis.greenhopper.jira:split_jira-agile-work-mode', location = 'includes/css/gh-sprint-filters.less' */
.ghx-sprint-meta{float:left;margin-right:10px}.ghx-sprint-meta .time{display:inline-block;line-height:2em;color:var(--ds-text-subtlest, #6b778c);margin-right:20px;margin-top:1px;cursor:pointer}.ghx-sprint-meta .time .days-left{margin-left:5px}.ghx-sprint-meta .complete-sprint-container .disabled{pointer-events:none}.ghx-sprint-dates{text-align:left;white-space:nowrap}.ghx-sprint-dates p+p{margin-top:5px}.ghx-sprint-dates.automanaged p:not(:last-child){margin-bottom:10px}